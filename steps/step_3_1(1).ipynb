   "source": [
    "import numpy as np\n",
    "\n",
    "w = np.array([2.5, -0.8, 3, 2, -0.15])\n",
    "x = np.array([1, 100, 50, 2, 500])\n",
    "y = -1\n",
    "\n",
    "M = w @ x * y\n",
    "P = 1 / (1 + np.exp(-M))\n",
    "P = round(float(P), 2)"
