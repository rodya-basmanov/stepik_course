   "source": [
    "import sympy as sp"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "cd887cbf",
   "metadata": {},
   "outputs": [],
   "source": [
    "x = sp.symbols('x')\n",
    "De = 0.3\n",
    "Dx = 0.8\n",
    "x0 = 11\n",
    "y1 = 10.6\n",
    "y2 = 11.2\n",
    "\n",
    "log_posterior = -1/(2*De) * ((x - y1)**2 + (x - y2)**2) - (x - x0)**2 / (2*Dx)\n",
    "\n",
    "df = sp.diff(log_posterior, x)\n",
    "\n",
    "solution = sp.solve(df, x)"
   ]
  },
  {
